<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="lab_layout">
        <!-- Multicompany -->
        <t t-if="o and 'company_id' in o">
            <t t-set="company" t-value="o.company_id"/>
        </t>
        <t t-if="not o or not 'company_id' in o">
            <t t-set="company" t-value="res_company"/>
        </t>

        <div class="header">
            <div class="row">
            </div>
        </div>
        <div class="article">
          <t t-raw="0" />
        </div>
    </template>
</odoo>
