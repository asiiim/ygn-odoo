<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_single_sms" model="ir.sequence">
            <field name="name">Single SMS</field>
            <field name="code">sms.single</field>
            <field name="prefix">SMS | </field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_multiple_sms" model="ir.sequence">
            <field name="name">Multiple SMS</field>
            <field name="code">sms.multiple</field>
            <field name="prefix">M-SMS | </field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
