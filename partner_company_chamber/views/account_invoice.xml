<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="add_chamber_in_account_invoice_search_view" model="ir.ui.view">
        <field name="name">add chamber in account invoice search view</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="account.view_account_invoice_filter"/>
        <field name="arch" type="xml">
            <search position="inside">
                 <field name="related_chamber" string="Chamber No."/>
                 <filter string="Chamber No." name='related_chamber' context="{'group_by':'related_chamber'}"/>
            </search>
        </field>
    </record>

</odoo>
